{
  "manifest_version": 3,
  "name": "My Frontpage",
  "version": "1.1.0",
  "description": "A custom new tab page with RSS news feeds",
  "homepage_url": "https://github.com/varunvs/my-firefox-frontpage",

  "browser_specific_settings": {
    "gecko": {
      "id": "my-firefox-frontpage@varunvs",
      "strict_min_version": "142.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },

  "permissions": [
    "storage",
    "downloads"
  ],

  "host_permissions": [
    "<all_urls>"
  ],

  "chrome_url_overrides": {
    "newtab": "newtab/newtab.html"
  },

  "options_ui": {
    "page": "options/options.html"
  },

  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  }
}
